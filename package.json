    "ecstatic": "~0.5.3",
    "gulp": "~3.8.1",
    "gulp-autoprefixer": "1.0.1",
    "gulp-jade": "~0.9.0",
    "gulp-livereload": "~2.1.0",
    "gulp-minify-css": "~0.3.5",
    "gulp-streamify": "0.0.5",
    "gulp-stylus": "1.3.3",
    "gulp-uglify": "~1.0.1",
    "gulp-util": "~3.0.1",
    "karma": "~0.12.21",
    "karma-chrome-launcher": "~0.1.4",
    "karma-coffee-preprocessor": "~0.2.1",
    "karma-jasmine": "~0.2.2",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "~2.0.0"
  },
  "browser": {},
  "browserify-shim": {},
  "browserify": {
